<h1>Registered members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Register no</th>
      <th>College name</th>
      <th>Dept name</th>
      <th>Mob no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.register_no %></td>
        <td><%= user.college_name %></td>
        <td><%= user.dept_name %></td>
        <td><%= user.mob_no %></td>
      </tr>
    <% end %>
  </tbody>
</table>
